[run]
source =
    urllib3
    test

[paths]
source =
    src/urllib3
    test

[report]
omit =
    src/urllib3/contrib/pyopenssl.py
    src/urllib3/contrib/securetransport.py
    src/urllib3/contrib/_securetransport/*

exclude_lines =
    except ModuleNotFoundError:
    except ImportError:
    pass
    import
    raise NotImplementedError
    .* # Platform-specific.*
    .*:.* # Python \d.*
    .* # Abstract
    .* # Defensive:
    if (?:typing.)?TYPE_CHECKING:
    ^\s*?\.\.\.\s*$
